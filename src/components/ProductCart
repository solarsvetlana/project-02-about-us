interface Props {
    id: number;
    title: string;
    price: number;
    description: string;
    image: string;
    category: string;

function ProductCard({ id, title, price, description, image, category }: Props) {
    const { id, title, price, description, image, category } = props;
    return <(
        <div className="product-card">
            <img src={image} alt={title} />
            <h2>{title}</h2>
            <p>{description}</p>
            <p>Price: ${price}</p>
            <p>Category: {category}</p>
        </div>
    );
}
export